     :fhir/extensionUri
     "http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-radiotherapy-modality",
     :type zen/map,
     :keys
     {:value
      {:fhir/polymorphic true,
       :zen.fhir/value-set
       {:symbol
        hl7-fhir-us-mcode.value-set.mcode-radiotherapy-modality-vs/value-set,
        :strength :required}}},
     :fhir/flags #{:MS}}},
   :performed
   {:fhir/polymorphic true,
    :type zen/map,
    :keys {:Period {:confirms #{hl7-fhir-r4-core.Period/schema}}}},
   :treatmentIntent
   {:confirms #{hl7-fhir-us-mcode.mcode-treatment-intent/schema},
    :fhir/extensionUri
    "http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-treatment-intent",
    :fhir/flags #{:MS}},
   :doseDeliveredToVolume
   {:type zen/vector,
    :every
    {:confirms
     #{hl7-fhir-us-mcode.mcode-radiotherapy-dose-delivered-to-volume/schema},
     :fhir/extensionUri
     "http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-radiotherapy-dose-delivered-to-volume",
     :fhir/flags #{:MS}}},
   :technique
   {:type zen/vector,
    :every
    {:confirms
     #{hl7-fhir-us-mcode.mcode-radiotherapy-technique/schema},
     :fhir/extensionUri
     "http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-radiotherapy-technique",
     :type zen/map,
     :keys
     {:value
      {:fhir/polymorphic true,
       :zen.fhir/value-set
       {:symbol
        hl7-fhir-us-mcode.value-set.mcode-radiotherapy-technique-vs/value-set,
        :strength :required}}},
     :fhir/flags #{:MS}}},
   :code
   {:match
    {:coding
     #{{:system
        "http://hl7.org/fhir/us/mcode/CodeSystem/mcode-resource-identifier-cs",
        :code "mcode-radiotherapy-course-summary"}}},
    :confirms #{hl7-fhir-r4-core.CodeableConcept/schema}},
   :bodySite
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
