  {:category
   {:match
    {:coding #{{:system "http://snomed.info/sct", :code "108290001"}}},
    :confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:MS}},
   :terminationReason
   {:confirms
    #{hl7-fhir-us-mcode.mcode-treatment-termination-reason/schema},
    :fhir/extensionUri
    "http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-treatment-termination-reason",
    :fhir/flags #{:MS}},
   :reasonCode
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:MS}}},
   :modality
